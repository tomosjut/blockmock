[
  {
    "name": "Demo: RabbitMQ Order Queue",
    "description": "RabbitMQ consumer die orders ontvangt en automatisch bevestigt",
    "protocol": "AMQP",
    "pattern": "REQUEST_REPLY",
    "enabled": true,
    "amqpConfig": {
      "brokerType": "RABBITMQ",
      "host": "localhost",
      "port": 5672,
      "virtualHost": "/",
      "operation": "CONSUME",
      "exchangeName": "demo.orders",
      "exchangeType": "TOPIC",
      "exchangeDurable": true,
      "exchangeAutoDelete": false,
      "queueName": "demo.order.processing",
      "queueDurable": true,
      "queueExclusive": false,
      "queueAutoDelete": false,
      "routingKey": "order.*",
      "bindingPattern": "order.*",
      "username": "guest",
      "password": "guest",
      "mockMessageContent": "{\"status\": \"order_confirmed\", \"order_id\": \"12345\", \"timestamp\": \"2025-11-17T10:00:00Z\"}",
      "autoReply": true,
      "replyDelayMs": 100
    }
  },
